{
  "name": "ncip-unified-system",
  "version": "1.0.0",
  "description": "NCIP Unified System with shared authentication and role-based routing",
  "main": "src/main.jsx",
  "scripts": {
    "dev": "vite",
    "dev:network": "vite --host 0.0.0.0",
    "sync-server": "node server/syncServer.js",
    "dev:full": "concurrently \"npm run dev:network\" \"npm run sync-server\"",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:network": "vite preview --host 0.0.0.0",
    "clean": "rm -rf node_modules package-lock.json"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@react-pdf/renderer": "^4.3.1",
    "axios": "^1.6.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "framer-motion": "^10.18.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.263.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.0",
    "vite": "^4.3.2"
  },
  "type": "module",
  "keywords": [
    "ncip",
